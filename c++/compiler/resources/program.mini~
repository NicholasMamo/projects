def fibonacci(x : int) : int {
	if ((x == 0) or (x == 1)) {
		return 1;
	} else {
		return fibonacci(x - 1) + fibonacci(x - 2);
	}
}

def fact(x : int) : int {
	if (x <= 1) {
		return 1;
	} else {
		return x * fact(x - 1);
	}
}

def max(x : real, y : real) : real {
	if (y < x) {
		return x;
	} else {
		return y;
	}
}

//write max(10, 20);
write fibonacci(5);

