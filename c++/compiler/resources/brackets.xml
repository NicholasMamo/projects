					</arithmetic_term>
				</assignment>
			</block>
			<block>
				<assignment>
					<identifier name = "y"></identifier>
					<arithmetic_term operator = "+">
						<integer>3</integer>
						<integer>1</integer>
					</arithmetic_term>
				</assignment>
			</block>
		</if>
	</statement>
</program>
